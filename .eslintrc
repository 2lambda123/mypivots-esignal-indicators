{
  "extends": [
      "guyellis",
    ],
  "env": {
  },
  "globals": {
      "setPriceStudy": true,
      "setShowCursorLabel": true,
      "setShowTitleParameters": true,
      "setCursorLabelName": true
  },
  "rules": {
    "comma-spacing": [1],
    "curly": [1],
    "eqeqeq": [1],
    "no-duplicate-case": [1],
    "no-eval": [1],
    "no-mixed-spaces-and-tabs": [1],
    "no-multi-spaces": [1],
    "no-redeclare": [1],
    "no-shadow": [1],
    "no-trailing-spaces": [1],
    "no-undef": [1],
    "no-unreachable": [1],
    "no-use-before-define": [1],
    "no-with": [0],
    "operator-assignment": [1],
    "quotes": [2, "double"],
    "radix": [1],
    "semi": [1]
  }
}
